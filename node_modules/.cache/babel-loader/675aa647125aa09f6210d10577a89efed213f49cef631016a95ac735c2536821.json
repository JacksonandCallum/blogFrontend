{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport mousedown from '@/utils/mousedown';\nexport default {\n  name: \"FrontLayout\",\n  data() {\n    return {\n      top: '',\n      notice: [],\n      categoryInfo: [],\n      user: JSON.parse(localStorage.getItem(\"USER\") || '{}'),\n      isHovered: false,\n      // Ê∑ªÂä†ÂèòÈáèÊéßÂà∂ÊÇ¨ÂÅúÁä∂ÊÄÅ\n      // top: '',\n      navShow: true,\n      // Á°Æ‰øùÂØºËà™Ê†èÂàùÂßãÂèØËßÅ\n      toolButton: false,\n      isDark: false,\n      mouseAnimation: false,\n      scrollTop: 0\n    };\n  },\n  computed: {},\n  watch: {\n    // ÁõëÂê¨topÂÄºÁöÑÂèòÂåñ\n    top(newValue, oldValue) {\n      // Á≠âÊñ∞ÂÄºÂ§ß‰∫é100ÁöÑÊó∂ÂÄôÂÜçÂÅöÂèòÂåñÔºà‰ºòÂåñ‰∏Ä‰∏ãÔºâ\n      if (newValue > 100) {\n        if (newValue > oldValue) {\n          this.navShow = false;\n          console.log('Âêë‰∏ãÊªöÂä®');\n        } else {\n          this.navShow = true;\n          console.log('Âêë‰∏äÊªöÂä®');\n        }\n      }\n    },\n    scrollTop(newVal, oldVal) {\n      this.toolButton = newVal > window.innerHeight / 3;\n    }\n  },\n  created() {\n    this.isDark = localStorage.getItem('darkMode') === 'true';\n    this.updateMode();\n  },\n  mounted() {\n    this.loadNotice();\n    this.loadCategory();\n    window.addEventListener('scroll', () => {\n      this.top = document.documentElement.scrollTop || document.body.scrollTop || window.scrollY;\n      this.scrollTop = this.top;\n      this.toolButton = this.scrollTop > window.innerHeight / 3;\n    });\n    if (this.isDaylight()) {\n      this.isDark = true;\n      let root = document.querySelector(\":root\");\n      root.style.setProperty(\"--background\", \"#272727\");\n      root.style.setProperty(\"--fontColor\", \"white\");\n      root.style.setProperty(\"--borderColor\", \"#4F4F4F\");\n      root.style.setProperty(\"--borderHoverColor\", \"black\");\n      root.style.setProperty(\"--articleFontColor\", \"#E4E4E4\");\n      root.style.setProperty(\"--articleGreyFontColor\", \"#D4D4D4\");\n      root.style.setProperty(\"--commentContent\", \"#D4D4D4\");\n      root.style.setProperty(\"--favoriteBg\", \"#1e1e1e\");\n    }\n    ;\n  },\n  destroyed() {},\n  methods: {\n    // Èº†Ê†áÊÇ¨ÂÅúÊó∂Ëß¶Âèë\n    handleMouseEnter() {\n      this.isHovered = true;\n    },\n    // Èº†Ê†áÁ¶ªÂºÄÊó∂Ëß¶Âèë\n    handleMouseLeave() {\n      this.isHovered = false;\n    },\n    loadNotice() {\n      this.$request.get('/notice/selectAll').then(res => {\n        this.notice = res.data;\n        let i = 0;\n        if (this.notice && this.notice.length) {\n          this.top = this.notice[0].content;\n          setInterval(() => {\n            this.top = this.notice[i].content;\n            i++;\n            if (i === this.notice.length) {\n              i = 0;\n            }\n          }, 2500);\n        }\n      });\n    },\n    updateUser() {\n      this.user = JSON.parse(localStorage.getItem('USER') || '{}'); // ÈáçÊñ∞Ëé∑Âèñ‰∏ãÁî®Êà∑ÁöÑÊúÄÊñ∞‰ø°ÊÅØ\n    },\n    // ÈÄÄÂá∫ÁôªÂΩï\n    logout() {\n      localStorage.removeItem(\"USER\");\n      location.href = \"/front/person\";\n    },\n    // Âà∞ÂêéÂè∞\n    toBack() {\n      setTimeout(() => {\n        this.$router.push(\"/login\");\n      }, 0);\n    },\n    // ÂàÜÁ±ª‰ø°ÊÅØ\n    loadCategory() {\n      this.$request.get(\"/category/selectAll\").then(res => {\n        this.categoryInfo = res.data;\n        console.log(res.data);\n      });\n    },\n    toTop() {\n      window.scrollTo({\n        top: 0,\n        behavior: \"smooth\"\n      });\n      console.log(\"ÂèëÂ∞Ñ~\");\n    },\n    isDaylight() {\n      let currDate = new Date();\n      if (currDate.getHours() > 22 || currDate.getHours() < 7) {\n        return true;\n      } else {\n        return false;\n      }\n    },\n    changeColor() {\n      this.isDark = !this.isDark;\n      if (this.isDark) {\n        localStorage.setItem('darkMode', this.isDark);\n        this.updateMode();\n      } else {\n        localStorage.setItem('darkMode', this.isDark);\n        this.updateMode();\n      }\n    },\n    updateMode() {\n      document.body.classList.toggle('dark-mode', this.isDark);\n    },\n    changeMouseAnimation() {\n      this.mouseAnimation = !this.mouseAnimation;\n      if (this.mouseAnimation) {\n        this.$nextTick(() => {\n          mousedown();\n        });\n      }\n    }\n  }\n};","map":{"version":3,"names":["mousedown","name","data","top","notice","categoryInfo","user","JSON","parse","localStorage","getItem","isHovered","navShow","toolButton","isDark","mouseAnimation","scrollTop","computed","watch","newValue","oldValue","console","log","newVal","oldVal","window","innerHeight","created","updateMode","mounted","loadNotice","loadCategory","addEventListener","document","documentElement","body","scrollY","isDaylight","root","querySelector","style","setProperty","destroyed","methods","handleMouseEnter","handleMouseLeave","$request","get","then","res","i","length","content","setInterval","updateUser","logout","removeItem","location","href","toBack","setTimeout","$router","push","toTop","scrollTo","behavior","currDate","Date","getHours","changeColor","setItem","classList","toggle","changeMouseAnimation","$nextTick"],"sources":["src/views/Front.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <!-- ÂÖ¨Âëä -->\r\n    <!-- <div class=\"front-notice\"><i class=\"el-icon-bell\" style=\"margin-right: 2px\"></i>ÂÖ¨ÂëäÔºö{{ top }}</div> -->\r\n    <!--Â§¥ÈÉ®-->\r\n    <!-- elËøáÊ∏°Âä®Áîª -->\r\n    <transition name=\"el-fade-in-linear\">\r\n      <div class=\"front-header\" id=\"nav-bar\" :class=\"[{ 'navbar-hover': isHovered }, navShow ? 'navOn' : 'navOff']\"\r\n        @mouseenter=\"handleMouseEnter\" @mouseleave=\"handleMouseLeave\">\r\n        <!-- Â∑¶Ëæπ -->\r\n        <div class=\"front-header-left\">\r\n          <!-- <img src=\"@/assets/imgs/logo.png\" alt=\"\"> -->\r\n          <div class=\"title\" style=\"color: white;\">\r\n            <span @click=\"$router.push({ path: '/front/home' })\">Mr Lv's BLOG</span>\r\n          </div>\r\n        </div>\r\n        <!-- ‰∏≠Èó¥ ÂØºËà™ÂàóË°® -->\r\n        <div>\r\n          <div class=\"front-header-center\">\r\n            <div class=\"front-header-nav\">\r\n              <el-menu :default-active=\"$route.path\" mode=\"horizontal\" router>\r\n                <el-menu-item index=\"/front/home\" class=\"active\">\r\n                  üè° <span>È¶ñÈ°µ</span>\r\n                </el-menu-item>\r\n                <!-- <el-menu-item index=\"/front/person\">‰∏™‰∫∫‰∏≠ÂøÉ</el-menu-item> -->\r\n                <el-menu-item class=\"active\">\r\n                  <el-dropdown :hide-timeout=\"500\" placement=\"bottom\">\r\n                    <li>\r\n                      <div class=\"my-menu\">\r\n                        üìí <span>ËÆ∞ÂΩï</span>\r\n                      </div>\r\n                    </li>\r\n                    <el-dropdown-menu slot=\"dropdown\" class=\"drop-items\">\r\n                      <el-dropdown-item v-for=\"category in categoryInfo\" :key=\"category.id\">\r\n                        <!-- @click=\"$router.push({ path: '/front/blogSort', query: { sortId: category.id } }) -->\r\n                        <div @click=\"$router.push('/front/blogSort?sortId=' + category.id)\">\r\n                          {{ category.name }}\r\n                        </div>\r\n                      </el-dropdown-item>\r\n                    </el-dropdown-menu>\r\n                  </el-dropdown>\r\n                </el-menu-item>\r\n                <el-menu-item index=\"/front/activityCenter\" class=\"active\">\r\n                  üß∞ <span>Ê¥ªÂä®‰∏≠ÂøÉ</span>\r\n                </el-menu-item>\r\n                <el-menu-item index=\"/front/about\" class=\"active\">\r\n                  üåè <span>ÂÖ≥‰∫éÊú¨ÁΩëÁ´ô</span>\r\n                </el-menu-item>\r\n                <el-menu-item class=\"active\">\r\n                  üì™ <span @click=\"toBack()\">ÂêéÂè∞</span>\r\n                </el-menu-item>\r\n              </el-menu>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <!-- Âè≥Ëæπ -->\r\n        <div class=\"front-header-right\">\r\n          <div>\r\n            <el-dropdown>\r\n              <div class=\"front-header-dropdown\" style=\"cursor: pointer;\">\r\n                <img :src=\"user.avatar\" alt=\"\" class=\"img\">\r\n                <div style=\"margin-left: 10px;color: black;\">\r\n                  <span style=\"color: aliceblue;\">{{ user.username }}</span>\r\n                </div>\r\n              </div>\r\n              <el-dropdown-menu slot=\"dropdown\" class=\"drop-items-right\">\r\n                <div v-if=\"!user.username\">\r\n                  <el-dropdown-item>\r\n                    <div style=\"text-decoration: none\" @click=\"$router.push('/front/person')\"\r\n                      v-if=\"$common.isEmpty($store.state.currentUser)\">\r\n                      <i class=\"fa fa-sign-in\" aria-hidden=\"true\"></i> <span>ÁôªÂΩï</span>\r\n                    </div>\r\n                  </el-dropdown-item>\r\n                </div>\r\n                <div v-else>\r\n                  <el-dropdown-item>\r\n                    <div style=\"text-decoration: none\" @click=\"$router.push('/front/person')\"\r\n                      v-if=\"!$common.isEmpty($store.state.currentUser)\">\r\n                      <i class=\"fa fa-user-circle\" aria-hidden=\"true\"></i> <span>‰∏™‰∫∫‰∏≠ÂøÉ</span>\r\n                    </div>\r\n                  </el-dropdown-item>\r\n                  <el-dropdown-item>\r\n                    <div style=\"text-decoration: none\" @click=\"logout\"\r\n                      v-if=\"!$common.isEmpty($store.state.currentUser)\">\r\n                      <i class=\"fa fa-sign-out\" aria-hidden=\"true\"></i> <span>ÈÄÄÂá∫</span>\r\n                    </div>\r\n                  </el-dropdown-item>\r\n                </div>\r\n\r\n              </el-dropdown-menu>\r\n            </el-dropdown>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </transition>\r\n\r\n    <div class=\"toolButton\">\r\n      <div class=\"backTop\" v-if=\"toolButton\" @click=\"toTop()\">\r\n        <!-- ÂõûÂà∞È°∂ÈÉ®ÊåâÈíÆ -->\r\n        <svg viewBox=\"0 0 1024 1024\" width=\"50\" height=\"50\">\r\n          <path\r\n            d=\"M696.741825 447.714002c2.717387-214.485615-173.757803-312.227566-187.33574-320.371729-10.857551 5.430775-190.050127 103.168727-187.33274 320.371729-35.297037 24.435488-73.306463 65.1623-67.875688 135.752376 5.430775 70.589076 76.018851 119.460051 103.168726 116.745664 27.152875-2.716387 19.004713-21.7221 19.004713-21.7221l8.148162-38.011425s40.721814 59.732525 51.583363 59.732525h146.609927c13.574938 0 51.585363-59.732525 51.585363-59.732525l8.147162 38.011425s-8.147162 19.005713 19.004713 21.7221c27.148876 2.714388 97.738951-46.156588 103.168727-116.745664s-32.57965-111.316888-67.876688-135.752376z m-187.33574-2.713388c-5.426776 0-70.589076-2.717387-78.733239-78.737238 2.713388-73.306463 73.306463-78.733239 78.733239-81.450626 5.430775 0 76.02385 8.144163 78.736238 81.450626-8.143163 76.019851-73.305463 78.737238-78.736238 78.737238z m0 0\"\r\n            fill=\"#000000\"></path>\r\n          <path\r\n            d=\"M423.602441 746.060699c6.47054-6.297579 12.823107-7.017417 21.629121-2.784372 34.520213 16.582259 70.232157 19.645568 107.031855 9.116944 8.118169-2.323476 15.974396-5.475765 23.598677-9.22392 13.712907-6.73648 26.003134 0.8878 26.080116 16.13936 0.109975 22.574907-0.024994 45.142816 0.080982 67.709725 0.031993 7.464316-2.277486 13.322995-9.44387 16.608254-7.277358 3.333248-13.765895 1.961558-19.526595-3.264264-3.653176-3.313253-7.063407-6.897444-10.634601-10.304675-6.563519-6.259588-6.676494-6.25259-10.625603 1.603638-8.437097 16.80121-16.821205 33.623415-25.257302 50.423625-2.489438 4.953882-5.706713 9.196925-11.411426 10.775569-8.355115 2.315478-15.772442-1.070758-20.272427-9.867774-8.774021-17.15313-17.269104-34.453228-25.918153-51.669344-3.750154-7.