{"ast":null,"code":"const proButton = () => import(\"@/components/proButton\");\nconst PersonPage = () => import(\"@/components/PersonPage.vue\");\nconst myBlogList = () => import(\"@/components/myBlogList.vue\");\nconst myActivityList = () => import(\"@/components/myActivityList.vue\");\nconst myLikeAndCollectBlogList = () => import(\"@/components/myLikeAndCollectBlogList.vue\");\nconst myLikeAndCollectActivityList = () => import(\"@/components/myLikeAndCollectActivityList.vue\");\nexport default {\n  components: {\n    proButton,\n    PersonPage,\n    myBlogList,\n    myActivityList,\n    myLikeAndCollectBlogList,\n    myLikeAndCollectActivityList\n  },\n  data() {\n    return {\n      currentUser: this.$store.state.currentUser,\n      username: \"\",\n      password: \"\",\n      // phoneNumber: \"\",\n      email: \"\",\n      loginParam: {\n        email: \"\",\n        password: \"\",\n        code: \"\"\n      },\n      // avatar: \"\",\n      showDialog: false,\n      code: \"\",\n      dialogTitle: \"\",\n      codeString: \"È™åËØÅÁ†Å\",\n      passwordFlag: null,\n      // intervalCode: null,\n      src: require(\"@/assets/imgs/boyWithCat.jpg\"),\n      login_register_src: require(\"@/assets/imgs/nice.jpg\"),\n      activeName: \"‰∏™‰∫∫ËµÑÊñô\",\n      blogs: [],\n      likeBlog: [],\n      collectBlog: [],\n      commentBlog: [],\n      stretch: false,\n      likesCurrent: \"ÂçöÂÆ¢\",\n      collectCurrent: \"ÂçöÂÆ¢\",\n      commentCurrent: \"ÂçöÂÆ¢\",\n      blogType: \"like\"\n    };\n  },\n  computed: {},\n  created() {\n    this.loadMyBlogs();\n    this.loadMyLikeBlogs();\n    this.loadCollectBlogs();\n    this.loadCommentBlogs();\n    // this.load1()\n  },\n  methods: {\n    // addPicture(res) {\n    //     this.avatar = res;\n    //     this.submitDialog()\n    // },\n    addBlog() {\n      window.open(\"/front/newBlog\");\n    },\n    loadMyBlogs() {\n      let userId = this.$store.state.currentUser.id;\n      this.$request.get(\"/blog/selectByUserId/\" + userId).then(res => {\n        console.log(\"-----------------------------------------\");\n        console.log(\"userId=\", userId);\n        this.blogs = res.data;\n        console.log(\"from Person(myBlogList):\", this.blogs);\n        console.log(\"-----------------------------------------\");\n      });\n    },\n    loadMyLikeBlogs() {\n      let userId = this.$store.state.currentUser.id;\n      console.log(\"Current blogType:\", this.blogType); // Ê£ÄÊü•blogTypeÁöÑÂÄº\n      if (this.blogType === \"like\") {\n        this.$request.get(\"/blog/selectLikesByUserId/\" + userId).then(res => {\n          console.log(\"-----------------------------------------\");\n          console.log(\"userId=\", userId);\n          this.likeBlog = res.data;\n          console.log(\"from Person(myLikeAndCollectBlogList--ÁÇπËµû):\", this.likeBlog);\n          console.log(\"-----------------------------------------\");\n        });\n      }\n    },\n    loadCollectBlogs() {\n      let userId = this.$store.state.currentUser.id;\n      this.blogType = \"collect\";\n      console.log(\"Current blogType:\", this.blogType); // Ê£ÄÊü•blogTypeÁöÑÂÄº\n      if (this.blogType === \"collect\") {\n        this.$request.get(\"/blog/selectCollectByUserId/\" + userId).then(res => {\n          console.log(\"-----------------------------------------\");\n          console.log(\"userId=\", userId);\n          this.collectBlog = res.data;\n          console.log(\"from Person(myLikeAndCollectBlogList--Êî∂Ëóè):\", this.collectBlog);\n          console.log(\"-----------------------------------------\");\n        }).catch(error => {\n          console.error(\"Error loading collected blogs:\", error); // Ê∑ªÂä†ÈîôËØØÂ§ÑÁêÜ\n        });\n      }\n    },\n    loadCommentBlogs() {\n      let userId = this.$store.state.currentUser.id;\n      this.blogType = \"comment\";\n      console.log(\"Current blogType:\", this.blogType); // Ê£ÄÊü•blogTypeÁöÑÂÄº\n      if (this.blogType === \"comment\") {\n        this.$request.get(\"/blog/selectCommentByUserId/\" + userId).then(res => {\n          console.log(\"-----------------------------------------\");\n          console.log(\"userId=\", userId);\n          this.commentBlog = res.data;\n          console.log(\"from Person(myLikeAndCollectBlogList--ËØÑËÆ∫):\", this.commentBlog);\n          console.log(\"-----------------------------------------\");\n        }).catch(error => {\n          console.error(\"Error loading collected blogs:\", error); // Ê∑ªÂä†ÈîôËØØÂ§ÑÁêÜ\n        });\n      }\n    },\n    // load1() {\n    //     let url\n    //     switch (this.blogType) {\n    //         case \"like\": url = \"/blog/selectLikesByUserId/\"; break\n    //         case \"collect\": url = \"/blog/selectCollectByUserId/\"; break\n    //         case \"comment\": url = \"/blog/selectLikesByUserId/\"; break\n    //     }\n    //     let userId = this.$store.state.currentUser.id\n    //     this.$request.get(url + userId).then(res => {\n    //         console.log(\"-----------------------------------------\")\n    //         console.log(\"userId=\", userId)\n    //         this.LikesAndCollects = res.data\n    //         console.log(\"from Person(myLikeAndCollectBlogList):\", (\"--\"+this.blogType+\"--\"), this.LikesAndCollects)\n    //         console.log(\"-----------------------------------------\")\n    //     })\n    // },\n    clickTab(tab) {\n      console.log(tab);\n    },\n    signUp() {\n      document.querySelector(\"#loginAndRegist\").classList.add('right-panel-active');\n    },\n    signIn() {\n      document.querySelector(\"#loginAndRegist\").classList.remove('right-panel-active');\n    },\n    login() {\n      if (this.$common.isEmpty(this.username) || this.$common.isEmpty(this.password)) {\n        this.$message.error(\"ËØ∑ËæìÂÖ•Ë¥¶Âè∑ÊàñÂØÜÁ†ÅÔºÅ\");\n        return;\n      }\n      let user = {\n        username: this.username.trim(),\n        // password: this.password.trim(),\n        role: \"USER\",\n        password: this.$common.decrypt(this.$common.encrypt(this.password.trim()))\n      };\n      this.$request.post('/login', user).then(res => {\n        if (!this.$common.isEmpty(res.data)) {\n          location.href = '/front/home';\n          localStorage.setItem(\"USER\", JSON.stringify(res.data)); // Â≠òÂÇ®Áî®Êà∑Êï∞ÊçÆ\n          this.username = \"\";\n          this.password = \"\";\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    },\n    regist() {\n      if (this.$common.isEmpty(this.username) || this.$common.isEmpty(this.password)) {\n        this.$message.error(\"ËØ∑ËæìÂÖ•Áî®Êà∑ÂêçÊàñÂØÜÁ†ÅÔºÅ\");\n        return;\n      }\n      if (this.dialogTitle === \"ÈÇÆÁÆ±È™åËØÅÁ†Å\" && this.$common.isEmpty(this.loginParam.email)) {\n        this.$message.error(\"ËØ∑ËæìÂÖ•ÈÇÆÁÆ±ÔºÅ\");\n        return false;\n      }\n      if (this.$common.isEmpty(this.loginParam.code)) {\n        this.$message.error(\"ËØ∑ËæìÂÖ•È™åËØÅÁ†ÅÔºÅ\");\n        return;\n      }\n      if (this.username.indexOf(\" \") !== -1 || this.password.indexOf(\" \") !== -1) {\n        this.$message.error(\"Áî®Êà∑ÂêçÊàñÂØÜÁ†Å‰∏çËÉΩÂåÖÂê´Á©∫Ê†ºÔºÅ\");\n        return;\n      }\n      let user = {\n        username: this.username.trim(),\n        code: this.loginParam.code.trim(),\n        password: this.$common.encrypt(this.loginParam.password.trim())\n      };\n      if (this.dialogTitle === \"ÈÇÆÁÆ±È™åËØÅÁ†Å\") {\n        user.email = this.loginParam.email;\n      }\n      this.$request.post(\"/user/register\", {\n        username: this.username,\n        password: this.loginParam.password,\n        email: this.loginParam.email,\n        code: this.loginParam.code\n      }).then(res => {\n        if (!this.$common.isEmpty(res.data)) {\n          this.$message.success(\"Ê≥®ÂÜåÊàêÂäü\");\n          this.username = \"\";\n          this.loginParam.password = \"\";\n          this.loginParam.email = \"\";\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    },\n    changeDialog(value) {\n      if (value === \"ÈÇÆÁÆ±È™åËØÅÁ†Å\") {\n        if (this.$common.isEmpty(this.loginParam.email)) {\n          this.$message.error(\"ËØ∑ËæìÂÖ•ÈÇÆÁÆ±ÔºÅ\");\n          return false;\n        }\n        if (!/^\\w+@[a-zA-Z0-9]{2,10}(?:\\.[a-z]{2,4}){1,3}$/.test(this.loginParam.email)) {\n          this.$message.error(\"ÈÇÆÁÆ±Ê†ºÂºèÊúâËØØÔºÅ\");\n          return false;\n        }\n      }\n      this.dialogTitle = value;\n      this.showDialog = true;\n    },\n    getCode() {\n      if (this.codeString === \"È™åËØÅÁ†Å\") {\n        if (this.dialogTitle === \"ÈÇÆÁÆ±È™åËØÅÁ†Å\") {\n          this.$request.post(\"/user/code/request\", {\n            email: this.loginParam.email\n          }).then(_ => {\n            console.log(\"È™åËØÅÁ†Å\", _);\n            this.$request.post(\"/user/code/email\", _.data).then(res => {\n              if (!this.$common.isEmpty(res.data)) {\n                // ÈÄöÁü•ÈÇÆÁÆ±ÂèëÈÄÅ\n                this.$notify({\n                  title: 'ÈÇÆÁÆ±È™åËØÅÁ†ÅÂ∑≤ÂèëÈÄÅ',\n                  message: 'È™åËØÅÁ†ÅÊúâÊïàÊó∂Èïø5ÂàÜÈíü, Â§±ÊïàËØ∑ÈáçÊñ∞ÂèëÈÄÅ',\n                  type: 'success',\n                  duration: 10 * 1000\n                });\n              }\n            });\n          });\n        } else {\n          this.$message.warning(\"ËØ∑Á®çÂêéÂÜçËØïÔºÅ\");\n        }\n      } else if (this.dialogTitle === \"ÊâæÂõûÂØÜÁ†Å\") {\n        // url = \"/user/getCodeForBind\";\n      }\n    },\n    submitDialog() {\n      if (this.dialogTitle === \"ÊâæÂõûÂØÜÁ†Å\") {\n        if (this.passwordFlag !== 1 && this.passwordFlag !== 2) {\n          this.$message.error(\"ËØ∑ÈÄâÊã©ÊâæÂõûÊñπÂºèÔºÅ\");\n        } else {\n          this.updateSecretInfo();\n        }\n      } else if (this.dialogTitle === \"ÈÇÆÁÆ±È™åËØÅÁ†Å\") {\n        this.showDialog = false;\n      }\n    },\n    updateSecretInfo() {\n      if (this.$common.isEmpty(this.loginParam.code)) {\n        this.$message.error(\"ËØ∑ËæìÂÖ•È™åËØÅÁ†ÅÔºÅ\");\n        return;\n      }\n      if (this.$common.isEmpty(this.loginParam.password)) {\n        this.$message.error(\"ËØ∑ËæìÂÖ•ÂØÜÁ†ÅÔºÅ\");\n        return;\n      }\n      let params = {\n        code: this.loginParam.code.trim(),\n        password: this.loginParam.password.trim()\n      };\n      if (this.dialogTitle === \"ÊâæÂõûÂØÜÁ†Å\") {\n        this.$http.post(this.$constant.baseURL + \"/user/updateForForgetPassword\", params, false, false).then(res => {\n          // this.clearDialog();\n          this.$message.success(\"‰øÆÊîπÊàêÂäüÔºåËØ∑ÈáçÊñ∞ÁôªÈôÜÔºÅ\");\n        }).catch(error => {\n          this.$message({\n            message: error.message,\n            type: \"error\"\n          });\n        });\n      } else {\n        this.$http.post(this.$constant.baseURL + \"/user/updateSecretInfo\", params, false, false).then(res => {\n          if (!this.$common.isEmpty(res.data)) {\n            this.$store.commit(\"loadCurrentUser\", res.data);\n            this.currentUser = this.$store.state.currentUser;\n            // this.clearDialog();\n            this.$message.success(\"‰øÆÊîπÊàêÂäüÔºÅ\");\n          }\n        }).catch(error => {\n          this.$message({\n            message: error.message,\n            type: \"error\"\n          });\n        });\n      }\n    }\n  }\n};","map":{"version":3,"names":["proButton","PersonPage","myBlogList","myActivityList","myLikeAndCollectBlogList","myLikeAndCollectActivityList","components","data","currentUser","$store","state","username","password","email","loginParam","code","showDialog","dialogTitle","codeString","passwordFlag","src","require","login_register_src","activeName","blogs","likeBlog","collectBlog","commentBlog","stretch","likesCurrent","collectCurrent","commentCurrent","blogType","computed","created","loadMyBlogs","loadMyLikeBlogs","loadCollectBlogs","loadCommentBlogs","methods","addBlog","window","open","userId","id","$request","get","then","res","console","log","catch","error","clickTab","tab","signUp","document","querySelector","classList","add","signIn","remove","login","$common","isEmpty","$message","user","trim","role","decrypt","encrypt","post","location","href","localStorage","setItem","JSON","stringify","msg","regist","indexOf","success","changeDialog","value","test","getCode","_","$notify","title","message","type","duration","warning","submitDialog","updateSecretInfo","params","$http","$constant","baseURL","commit"],"sources":["src/views/front/Person.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <!-- ÁôªÈôÜÂíåÊ≥®ÂÜå -->\r\n    <div v-if=\"this.$common.isEmpty(currentUser)\" class=\"myCenter in-up-container my-animation-hideToShow\">\r\n      <!-- ËÉåÊôØÂõæÁâá -->\r\n      <el-image class=\"my-el-image\" style=\"position: absolute\" v-once lazy :src=\"login_register_src\" fit=\"cover\">\r\n        <div slot=\"error\" class=\"image-slot\"></div>\r\n      </el-image>\r\n      <div class=\"in-up\" id=\"loginAndRegist\">\r\n        <div class=\"form-container sign-up-container\">\r\n          <div class=\"myCenter\">\r\n            <h1>Ê≥®ÂÜå</h1>\r\n            <input v-model=\"username\" type=\"text\" maxlength=\"30\" placeholder=\"Áî®Êà∑Âêç\">\r\n            <input v-model=\"loginParam.password\" type=\"password\" maxlength=\"30\" placeholder=\"ÂØÜÁ†Å\">\r\n            <input v-model=\"loginParam.email\" type=\"email\" placeholder=\"ÈÇÆÁÆ±\">\r\n            <input v-model=\"loginParam.code\" type=\"text\" placeholder=\"È™åËØÅÁ†Å\" disabled>\r\n            <a style=\"margin: 0\" href=\"#\" @click=\"changeDialog('ÈÇÆÁÆ±È™åËØÅÁ†Å')\">Ëé∑ÂèñÈ™åËØÅÁ†Å</a>\r\n            <button @click=\"regist()\">Ê≥®ÂÜå</button>\r\n          </div>\r\n        </div>\r\n        <div class=\"form-container sign-in-container\">\r\n          <div class=\"myCenter\">\r\n            <h1>ÁôªÂΩï</h1>\r\n            <input v-model=\"username\" type=\"text\" placeholder=\"Áî®Êà∑Âêç/ÈÇÆÁÆ±/ÊâãÊú∫Âè∑\">\r\n            <input v-model=\"password\" type=\"password\" placeholder=\"ÂØÜÁ†Å\">\r\n            <a href=\"#\" @click=\"changeDialog('ÊâæÂõûÂØÜÁ†Å')\">ÂøòËÆ∞ÂØÜÁ†ÅÔºü</a>\r\n            <button @click=\"login()\">ÁôªÂΩï</button>\r\n          </div>\r\n        </div>\r\n        <div class=\"overlay-container\">\r\n          <div class=\"overlay\">\r\n            <div class=\"overlay-panel myCenter overlay-left\">\r\n              <h1>Â∑≤ÊúâÂ∏êÂè∑Ôºü</h1>\r\n              <p>ËØ∑ÁôªÂΩïüöÄ</p>\r\n              <button class=\"ghost\" @click=\"signIn()\">ÁôªÂΩï</button>\r\n            </div>\r\n            <div class=\"overlay-panel myCenter overlay-right\">\r\n              <h1>Ê≤°ÊúâÂ∏êÂè∑Ôºü</h1>\r\n              <p>Á´ãÂç≥Ê≥®ÂÜåÂêßüòÉ</p>\r\n              <button class=\"ghost\" @click=\"signUp()\">Ê≥®ÂÜå</button>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Áî®Êà∑‰ø°ÊÅØ -->\r\n    <div v-else class=\"user-container myCenter my-animation-hideToShow\">\r\n      <!-- ËÉåÊôØÂõæÁâá -->\r\n      <el-image class=\"my-el-image\" style=\"position: absolute\" v-once lazy :src=\"src\" fit=\"cover\">\r\n        <div slot=\"error\" class=\"image-slot\"></div>\r\n      </el-image>\r\n      <div class=\"shadow-box-mini user-info\" style=\"display: flex\">\r\n        <div class=\"user-left\">\r\n          <div>\r\n            <el-tabs v-model=\"activeName\" @tab-click=\"clickTab\" stretch=\"stretch\">\r\n              <el-tab-pane label=\"‰∏™‰∫∫ËµÑÊñô\" name=\"‰∏™‰∫∫ËµÑÊñô\">\r\n                <!-- ‰∏™‰∫∫ËµÑÊñô -->\r\n                <PersonPage :currentUser=\"currentUser\"></PersonPage>\r\n              </el-tab-pane>\r\n              <el-tab-pane label=\"ÊàëÂèëË°®ÁöÑÂçöÂÆ¢\" name=\"ÊàëÂèëË°®ÁöÑÂçöÂÆ¢\">\r\n                <!-- ÊàëÂèëË°®ÁöÑÂçöÂÆ¢ -->\r\n                <div style=\"margin-top: 30px;margin-bottom: 30px;\">\r\n                  <proButton :info=\"'ÂèëË°®ÂçöÂÆ¢'\" :before=\"'rgb(131, 123, 199)'\"\r\n                    :after=\"'linear-gradient(45deg, #f43f3b, #ec008c)'\" @click.native=\"addBlog()\">\r\n                  </proButton>\r\n                </div>\r\n                <div>\r\n                  <myBlogList :blogs=\"blogs\"></myBlogList>\r\n                </div>\r\n              </el-tab-pane>\r\n              <el-tab-pane label=\"ÊàëÊä•ÂêçÁöÑÊ¥ªÂä®\" name=\"ÊàëÊä•ÂêçÁöÑÊ¥ªÂä®\">\r\n                <!-- ÊàëÊä•ÂêçÁöÑÊ¥ªÂä® -->\r\n                <div>\r\n                  <myActivityList></myActivityList>\r\n                </div>\r\n              </el-tab-pane>\r\n              <el-tab-pane label=\"ÊàëÁöÑÁÇπËµû\" name=\"ÊàëÁöÑÁÇπËµû\">\r\n                <!-- ÊàëÁöÑÁÇπËµû -->\r\n                <div style=\"display: flex;flex-direction: row;margin-top: 30px;\">\r\n                  <div style=\"margin: auto 5px;\">\r\n                    <proButton :info=\"'ÂçöÂÆ¢'\" :before=\"'rgb(131, 123, 199)'\"\r\n                      :after=\"'linear-gradient(45deg, #f43f3b, #ec008c)'\" @click.native=\"likesCurrent = 'ÂçöÂÆ¢'\">\r\n                    </proButton>\r\n                  </div>\r\n                  <div style=\"margin: auto 5px;\">\r\n                    <proButton :info=\"'Ê¥ªÂä®'\" :before=\"'rgb(131, 123, 199)'\"\r\n                      :after=\"'linear-gradient(45deg, #f43f3b, #ec008c)'\" @click.native=\"likesCurrent = 'Ê¥ªÂä®'\">\r\n                    </proButton>\r\n                  </div>\r\n                </div>\r\n                <div>\r\n                  <myLikeAndCollectBlogList :blogs=\"likeBlog\" :tip=\"'ÁÇπËµû'\" blogType=\"like\" v-if=\"likesCurrent === 'ÂçöÂÆ¢'\"\r\n                    style=\"margin-top: 30px;\">\r\n                  </myLikeAndCollectBlogList>\r\n                </div>\r\n                <div>\r\n                  <myLikeAndCollectActivityList v-if=\"likesCurrent === 'Ê¥ªÂä®'\" type=\"like\" :tip=\"'ÁÇπËµû'\">\r\n                  </myLikeAndCollectActivityList>\r\n                </div>\r\n              </el-tab-pane>\r\n              <el-tab-pane label=\"ÊàëÁöÑÊî∂Ëóè\" name=\"ÊàëÁöÑÊî∂Ëóè\">\r\n                <!-- ÊàëÁöÑÊî∂Ëóè -->\r\n                <div style=\"display: flex;flex-direction: row;margin-top: 30px;\">\r\n                  <div style=\"margin: auto 5px;\">\r\n                    <proButton :info=\"'ÂçöÂÆ¢'\" :before=\"'rgb(131, 123, 199)'\"\r\n                      :after=\"'linear-gradient(45deg, #f43f3b, #ec008c)'\" @click.native=\"collectCurrent = 'ÂçöÂÆ¢'\">\r\n                    </proButton>\r\n                  </div>\r\n                  <div style=\"margin: auto 5px;\">\r\n                    <proButton :info=\"'Ê¥ªÂä®'\" :before=\"'rgb(131, 123, 199)'\"\r\n                      :after=\"'linear-gradient(45deg, #f43f3b, #ec008c)'\" @click.native=\"collectCurrent = 'Ê¥ªÂä®'\">\r\n                    </proButton>\r\n                  </div>\r\n                </div>\r\n                <div>\r\n                  <myLikeAndCollectBlogList :blogs=\"collectBlog\" :tip=\"'Êî∂Ëóè'\" blogType=\"collect\"\r\n                    v-if=\"collectCurrent === 'ÂçöÂÆ¢'\" style=\"margin-top: 30px;\">\r\n                  </myLikeAndCollectBlogList>\r\n                </div>\r\n                <div>\r\n                  <myLikeAndCollectActivityList v-if=\"collectCurrent === 'Ê¥ªÂä®'\" type=\"collect\" :tip=\"'Êî∂Ëóè'\">\r\n                  </myLikeAndCollectActivityList>\r\n                </div>\r\n              </el-tab-pane>\r\n              <el-tab-pane label=\"ÊàëÁöÑËØÑËÆ∫\" name=\"ÊàëÁöÑËØÑËÆ∫\">\r\n                <!-- ÊàëÁöÑËØÑËÆ∫ -->\r\n                <div style=\"display: flex;flex-direction: row;margin-top: 30px;\">\r\n                  <div style=\"margin: auto 5px;\">\r\n                    <proButton :info=\"'ÂçöÂÆ¢'\" :before=\"'rgb(131, 123, 199)'\"\r\n                      :after=\"'linear-gradient(45deg, #f43f3b, #ec008c)'\" @click.native=\"commentCurrent = 'ÂçöÂÆ¢'\">\r\n                    </proButton>\r\n                  </div>\r\n                  <div style=\"margin: auto 5px;\">\r\n                    <proButton :info=\"'Ê¥ªÂä®'\" :before=\"'rgb(131, 123, 199)'\"\r\n                      :after=\"'linear-gradient(45deg, #f43f3b, #ec008c)'\" @click.native=\"commentCurrent = 'Ê¥ªÂä®'\">\r\n                    </proButton>\r\n                  </div>\r\n                </div>\r\n                <div>\r\n                  <myLikeAndCollectBlogList :blogs=\"commentBlog\" :tip=\"'ËØÑËÆ∫'\" blogType=\"comment\"\r\n                    v-if=\"commentCurrent === 'ÂçöÂÆ¢'\" style=\"margin-top: 30px;\">\r\n                  </myLikeAndCollectBlogList>\r\n                </div>\r\n                <div>\r\n                  <myLikeAndCollectActivityList v-if=\"commentCurrent === 'Ê¥ªÂä®'\" type=\"comment\" :tip=\"'ËØÑËÆ∫'\">\r\n                  </myLikeAndCollectActivityList>\r\n                </div>\r\n              </el-tab-pane>\r\n            </el-tabs>\r\n          </div>\r\n        </div>\r\n        <!-- <div class=\"user-right\"></div> -->\r\n      </div>\r\n    </div>\r\n\r\n\r\n    <!-- ÂºπÁ™ó -->\r\n    <el-dialog :title=\"dialogTitle\" :visible.sync=\"showDialog\" width=\"30%\" :append-to-body=\"true\"\r\n      :close-on-click-modal=\"false\" center>\r\n      <div class=\"myCenter\" style=\"flex-direction: column\">\r\n        <div>\r\n          <div v-if=\"dialogTitle === 'ÊâæÂõûÂØÜÁ†Å'\">\r\n            <div class=\"myCenter\" style=\"margin-bottom: 12px\">\r\n              <el-radio-group v-model=\"passwordFlag\">\r\n                <el-radio :label=\"1\" style=\"margin-right: 10px\">ÊâãÊú∫Âè∑</el-radio>\r\n                <el-radio :label=\"2\">ÈÇÆÁÆ±</el-radio>\r\n              </el-radio-group>\r\n            </div>\r\n            <div v-if=\"passwordFlag === 1\">\r\n              <div style=\"margin-bottom: 5px\">ÊâãÊú∫Âè∑Ôºö</div>\r\n              <el-input v-model=\"phone\"></el-input>\r\n              <div style=\"margin-top: 10px;margin-bottom: 5px\">È™åËØÅÁ†ÅÔºö</div>\r\n              <el-input v-model=\"code\"></el-input>\r\n              <div style=\"margin-top: 10px;margin-bottom: 5px\">Êñ∞ÂØÜÁ†ÅÔºö</div>\r\n              <el-input maxlength=\"30\" v-model=\"password\"></el-input>\r\n            </div>\r\n            <div v-else-if=\"passwordFlag === 2\">\r\n              <div style=\"margin-bottom: 5px\">ÈÇÆÁÆ±Ôºö</div>\r\n              <el-input v-model=\"email\"></el-input>\r\n              <div style=\"margin-top: 10px;margin-bottom: 5px\">È™åËØÅÁ†ÅÔºö</div>\r\n              <el-input v-model=\"code\"></el-input>\r\n              <div style=\"margin-top: 10px;margin-bottom: 5px\">Êñ∞ÂØÜÁ†ÅÔºö</div>\r\n              <el-input maxlength=\"30\" v-model=\"password\"></el-input>\r\n            </div>\r\n          </div>\r\n          <div v-else-if=\"dialogTitle === 'ÈÇÆÁÆ±È™åËØÅÁ†Å'\">\r\n            <div>\r\n              <div style=\"margin-bottom: 5px\">ÈÇÆÁÆ±Ôºö</div>\r\n              <el-input v-model=\"loginParam.email\"></el-input>\r\n              <div style=\"margin-top: 10px;margin-bottom: 5px\">È™åËØÅÁ†ÅÔºö</div>\r\n              <el-input v-model=\"code\"></el-input>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div style=\"display: flex;margin-top: 30px\">\r\n          <proButton :info=\"codeString\" v-show=\"dialogTitle === 'ÊâæÂõûÂØÜÁ†Å' || dialogTitle === 'ÈÇÆÁÆ±È™åËØÅÁ†Å'\"\r\n            @click.native=\"getCode()\" :before=\"$constant.before_color_1\" :after=\"$constant.after_color_1\"\r\n            style=\"margin-right: 20px\">\r\n          </proButton>\r\n          <proButton :info=\"'Êèê‰∫§'\" @click.native=\"submitDialog()\" :before=\"$constant.before_color_2\"\r\n            :after=\"$constant.after_color_2\">\r\n          </proButton>\r\n        </div>\r\n      </div>\r\n    </el-dialog>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nconst proButton = () => import(\"@/components/proButton\");\r\nconst PersonPage = () => import(\"@/components/PersonPage.vue\")\r\nconst myBlogList = () => import(\"@/components/myBlogList.vue\")\r\nconst myActivityList = () => import(\"@/components/myActivityList.vue\")\r\nconst myLikeAndCollectBlogList = () => import(\"@/components/myLikeAndCollectBlogList.vue\")\r\nconst myLikeAndCollectActivityList = () => import(\"@/components/myLikeAndCollectActivityList.vue\")\r\n\r\nexport default {\r\n  components: {\r\n    proButton,\r\n    PersonPage,\r\n    myBlogList,\r\n    myActivityList,\r\n    myLikeAndCollectBlogList,\r\n    myLikeAndCollectActivityList\r\n  },\r\n  data() {\r\n    return {\r\n      currentUser: this.$store.state.currentUser,\r\n      username: \"\",\r\n      password: \"\",\r\n      // phoneNumber: \"\",\r\n      email: \"\",\r\n      loginParam: {\r\n        email: \"\",\r\n        password: \"\",\r\n        code: \"\"\r\n      },\r\n      // avatar: \"\",\r\n      showDialog: false,\r\n      code: \"\",\r\n      dialogTitle: \"\",\r\n      codeString: \"È™åËØÅÁ†Å\",\r\n      passwordFlag: null,\r\n      // intervalCode: null,\r\n      src: require(\"@/assets/imgs/boyWithCat.jpg\"),\r\n      login_register_src: require(\"@/assets/imgs/nice.jpg\"),\r\n      activeName: \"‰∏™‰∫∫ËµÑÊñô\",\r\n      blogs: [],\r\n      likeBlog: [],\r\n      collectBlog: [],\r\n      commentBlog: [],\r\n      stretch: false,\r\n      likesCurrent: \"ÂçöÂÆ¢\",\r\n      collectCurrent: \"ÂçöÂÆ¢\",\r\n      commentCurrent: \"ÂçöÂÆ¢\",\r\n      blogType: \"like\"\r\n    }\r\n  },\r\n  computed: {},\r\n  created() {\r\n    this.loadMyBlogs()\r\n    this.loadMyLikeBlogs()\r\n    this.loadCollectBlogs()\r\n    this.loadCommentBlogs()\r\n    // this.load1()\r\n  },\r\n  methods: {\r\n    // addPicture(res) {\r\n    //     this.avatar = res;\r\n    //     this.submitDialog()\r\n    // },\r\n    addBlog() {\r\n      window.open(\"/front/newBlog\")\r\n    },\r\n    loadMyBlogs() {\r\n      let userId = this.$store.state.currentUser.id\r\n      this.$request.get(\"/blog/selectByUserId/\" + userId).then(res => {\r\n        console.log(\"-----------------------------------------\")\r\n        console.log(\"userId=\", userId)\r\n        this.blogs = res.data\r\n        console.log(\"from Person(myBlogList):\", this.blogs)\r\n        console.log(\"-----------------------------------------\")\r\n      })\r\n    },\r\n    loadMyLikeBlogs() {\r\n      let userId = this.$store.state.currentUser.id\r\n      console.log(\"Current blogType:\", this.blogType); // Ê£ÄÊü•blogTypeÁöÑÂÄº\r\n      if (this.blogType === \"like\") {\r\n        this.$request.get(\"/blog/selectLikesByUserId/\" + userId).then(res => {\r\n          console.log(\"-----------------------------------------\")\r\n          console.log(\"userId=\", userId)\r\n          this.likeBlog = res.data\r\n          console.log(\"from Person(myLikeAndCollectBlogList--ÁÇπËµû):\", this.likeBlog)\r\n          console.log(\"-----------------------------------------\")\r\n        })\r\n      }\r\n    },\r\n    loadCollectBlogs() {\r\n      let userId = this.$store.state.currentUser.id\r\n      this.blogType = \"collect\"\r\n      console.log(\"Current blogType:\", this.blogType); // Ê£ÄÊü•blogTypeÁöÑÂÄº\r\n      if (this.blogType === \"collect\") {\r\n        this.$request.get(\"/blog/selectCollectByUserId/\" + userId).then(res => {\r\n          console.log(\"-----------------------------------------\")\r\n          console.log(\"userId=\", userId)\r\n          this.collectBlog = res.data\r\n          console.log(\"from Person(myLikeAndCollectBlogList--Êî∂Ëóè):\", this.collectBlog)\r\n          console.log(\"-----------------------------------------\")\r\n        }).catch(error => {\r\n          console.error(\"Error loading collected blogs:\", error); // Ê∑ªÂä†ÈîôËØØÂ§ÑÁêÜ\r\n        });\r\n      }\r\n    },\r\n    loadCommentBlogs() {\r\n      let userId = this.$store.state.currentUser.id\r\n      this.blogType = \"comment\"\r\n      console.log(\"Current blogType:\", this.blogType); // Ê£ÄÊü•blogTypeÁöÑÂÄº\r\n      if (this.blogType === \"comment\") {\r\n        this.$request.get(\"/blog/selectCommentByUserId/