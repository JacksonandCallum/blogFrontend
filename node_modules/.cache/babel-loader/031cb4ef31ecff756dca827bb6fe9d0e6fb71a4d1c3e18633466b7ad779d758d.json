{"ast":null,"code":"import linkbarContent from \"../../components/linkbar-content.vue\";\nimport dropdownSelect from \"../../components/dropdown-select\";\nconst temp = (name, link) => {\n  return `\n  <div style=\"padding:10px 0\">\n    <div\n      style=\"\n        width: 300px;\n        padding: 15px;\n        border-radius: 5px;\n        padding:10px;\n        margin:auto;\n        background: #f5f5f5;\n        cursor: pointer;\n      \"\n      onclick=\"window.open('${link}')\"\n      class=\"link-block-item\"\n    >${name}</div>\n  </div>\n   \n  `;\n};\nexport default {\n  name: \"link-block\",\n  components: {\n    linkbarContent,\n    dropdownSelect\n  },\n  props: {\n    content: {\n      default() {\n        return {};\n      }\n    },\n    token: {\n      default: \"\"\n    }\n  },\n  data() {\n    return {\n      name: \"\",\n      link: \"\",\n      show: false\n    };\n  },\n  watch: {\n    token: {\n      immediate: true,\n      handler() {\n        this.name = this.content.name;\n        this.link = this.content.link;\n        this.onChange();\n      }\n    }\n  },\n  mounted() {},\n  methods: {\n    setLink(data) {\n      this.link = data.link;\n      this.name = data.name;\n      this.onChange();\n    },\n    onChange() {\n      this.$emit(\"change\", {\n        link: this.link,\n        name: this.name,\n        template: temp(this.name, this.link)\n      });\n      this.show = false;\n    }\n  }\n};","map":{"version":3,"names":["linkbarContent","dropdownSelect","temp","name","link","components","props","content","default","token","data","show","watch","immediate","handler","onChange","mounted","methods","setLink","$emit","template"],"sources":["node_modules/vue2-lay-editor/temp/linkBlock/linkBlock.vue"],"sourcesContent":["<template>\n  <div class=\"link-block\">\n    <div class=\"link-block-section\">\n      <dropdown-select v-model=\"show\">\n        <!-- 展示 -->\n        <div slot=\"title\">\n          <div class=\"link-block-item\">\n            <p class=\"title\">\n              {{ name }}\n            </p>\n          </div>\n        </div>\n        <!-- 操作 -->\n        <div>\n          <linkbar-content\n            :link=\"link\"\n            :name=\"name\"\n            :autoClear=\"false\"\n            @confirm=\"setLink\"\n          ></linkbar-content>\n        </div>\n      </dropdown-select>\n    </div>\n  </div>\n</template>\n\n<script>\nimport linkbarContent from \"../../components/linkbar-content.vue\";\nimport dropdownSelect from \"../../components/dropdown-select\";\n\nconst temp = (name, link) => {\n  return `\n  <div style=\"padding:10px 0\">\n    <div\n      style=\"\n        width: 300px;\n        padding: 15px;\n        border-radius: 5px;\n        padding:10px;\n        margin:auto;\n        background: #f5f5f5;\n        cursor: pointer;\n      \"\n      onclick=\"window.open('${link}')\"\n      class=\"link-block-item\"\n    >${name}</div>\n  </div>\n   \n  `;\n};\n\nexport default {\n  name: \"link-block\",\n  components: {\n    linkbarContent,\n    dropdownSelect,\n  },\n  props: {\n    content: {\n      default() {\n        return {};\n      },\n    },\n    token: {\n      default: \"\",\n    },\n  },\n  data() {\n    return {\n      name: \"\",\n      link: \"\",\n      show: false,\n    };\n  },\n  watch: {\n    token: {\n      immediate: true,\n      handler() {\n        this.name = this.content.name;\n        this.link = this.content.link;\n        this.onChange();\n      },\n    },\n  },\n  mounted() {},\n  methods: {\n    setLink(data) {\n      this.link = data.link;\n      this.name = data.name;\n      this.onChange();\n    },\n    onChange() {\n      this.$emit(\"change\", {\n        link: this.link,\n        name: this.name,\n        template: temp(this.name, this.link),\n      });\n      this.show = false;\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n.link-block {\n  padding: 20px 10px;\n  // background: #fff;\n}\n// 内容 区域\n.link-block-section {\n  display: flex;\n  justify-content: center;\n}\n.link-block-section .link-block-item {\n  width: 300px;\n  padding: 15px;\n  border-radius: 5px;\n  margin: auto;\n  background: #f5f5f5;\n  cursor: pointer;\n}\n.link-block-section .link-block-item .title {\n  margin: 0;\n  line-height: 18px;\n  font-size: 14px;\n  color: #555;\n}\n</style>\n"],"mappings":"AA2BA,OAAAA,cAAA;AACA,OAAAC,cAAA;AAEA,MAAAC,IAAA,GAAAA,CAAAC,IAAA,EAAAC,IAAA;EACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA,8BAAAA,IAAA;AACA;AACA,OAAAD,IAAA;AACA;;AAEA;AACA;AAEA;EACAA,IAAA;EACAE,UAAA;IACAL,cAAA;IACAC;EACA;EACAK,KAAA;IACAC,OAAA;MACAC,QAAA;QACA;MACA;IACA;IACAC,KAAA;MACAD,OAAA;IACA;EACA;EACAE,KAAA;IACA;MACAP,IAAA;MACAC,IAAA;MACAO,IAAA;IACA;EACA;EACAC,KAAA;IACAH,KAAA;MACAI,SAAA;MACAC,QAAA;QACA,KAAAX,IAAA,QAAAI,OAAA,CAAAJ,IAAA;QACA,KAAAC,IAAA,QAAAG,OAAA,CAAAH,IAAA;QACA,KAAAW,QAAA;MACA;IACA;EACA;EACAC,QAAA;EACAC,OAAA;IACAC,QAAAR,IAAA;MACA,KAAAN,IAAA,GAAAM,IAAA,CAAAN,IAAA;MACA,KAAAD,IAAA,GAAAO,IAAA,CAAAP,IAAA;MACA,KAAAY,QAAA;IACA;IACAA,SAAA;MACA,KAAAI,KAAA;QACAf,IAAA,OAAAA,IAAA;QACAD,IAAA,OAAAA,IAAA;QACAiB,QAAA,EAAAlB,IAAA,MAAAC,IAAA,OAAAC,IAAA;MACA;MACA,KAAAO,IAAA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}