{"ast":null,"code":"import dropdownSelect from \"../../components/dropdown-select.vue\";\nimport config from \"../../config/index\";\nimport emoteSymbol from \"../utils/emoteSymbol\";\nexport default {\n  name: \"WorkspaceJsonAlignMode\",\n  components: {\n    dropdownSelect\n  },\n  data() {\n    return {\n      isOpen: false,\n      checked: 0,\n      currentList: [],\n      emoticonList: emoteSymbol,\n      config\n    };\n  },\n  mounted() {\n    this.currentList = emoteSymbol[0].list;\n  },\n  methods: {\n    switchTabs(item, index) {\n      this.checked = index;\n      this.currentList = item.list;\n    },\n    execCommand(item) {\n      document.execCommand(\"insertHTML\", false, `<a style=\"font-size: 22px;line-height:16px;position: relative;top: 1px\">${item}</a><span>&nbsp</span>`);\n    }\n  }\n};","map":{"version":3,"names":["dropdownSelect","config","emoteSymbol","name","components","data","isOpen","checked","currentList","emoticonList","mounted","list","methods","switchTabs","item","index","execCommand","document"],"sources":["node_modules/vue2-lay-editor/edit-tools/components/emoteSymbol.vue"],"sourcesContent":["<template>\n  <div class=\"emote-symbol\" @mousedown.prevent>\n    <dropdown-select v-model=\"isOpen\">\n      <!-- 展示 -->\n      <div slot=\"title\">\n        <div class=\"head\">\n          <!-- 表情符号 -->\n          <i class=\"iconfont icon-a-1\"></i>\n        </div>\n      </div>\n      <!-- 操作 -->\n      <div class=\"dropdown-panel\">\n        <div class=\"list\">\n          <div\n            @click=\"execCommand(item)\"\n            v-for=\"(item, index) in currentList\"\n            :key=\"index\"\n            class=\"emote-item\"\n          >\n            {{ item }}\n          </div>\n        </div>\n\n        <div class=\"tabs\" :style=\"{ '--theme': config.themeColor }\">\n          <span\n            @click=\"switchTabs(item, index)\"\n            :class=\"[{ active: index == checked }]\"\n            v-for=\"(item, index) in emoticonList\"\n            :key=\"index\"\n            >{{ item.title }}</span\n          >\n        </div>\n      </div>\n    </dropdown-select>\n  </div>\n</template>\n\n<script>\nimport dropdownSelect from \"../../components/dropdown-select.vue\";\nimport config from \"../../config/index\";\nimport emoteSymbol from \"../utils/emoteSymbol\";\nexport default {\n  name: \"WorkspaceJsonAlignMode\",\n  components: {\n    dropdownSelect,\n  },\n  data() {\n    return {\n      isOpen: false,\n      checked: 0,\n      currentList: [],\n      emoticonList: emoteSymbol,\n      config,\n    };\n  },\n  mounted() {\n    this.currentList = emoteSymbol[0].list;\n  },\n  methods: {\n    switchTabs(item, index) {\n      this.checked = index;\n      this.currentList = item.list;\n    },\n    execCommand(item) {\n      document.execCommand(\n        \"insertHTML\",\n        false,\n        `<a style=\"font-size: 22px;line-height:16px;position: relative;top: 1px\">${item}</a><span>&nbsp</span>`\n      );\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n.emote-symbol .head {\n  width: 30px;\n  height: 30px;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n}\n\n.emote-symbol .head:hover {\n  background: #f5f5f5;\n}\n\n.emote-symbol .dropdown-panel {\n  width: 365px;\n  height: 310px;\n  background: rgba(255, 255, 255, 0.7);\n  box-shadow: 0 5px 20px 0 #e4e8eb;\n  display: flex;\n  flex-direction: column;\n  margin-top: 10px;\n}\n.emote-symbol .dropdown-panel .list {\n  flex: 1;\n  overflow: auto;\n  display: flex;\n  flex-wrap: wrap;\n  align-content: baseline;\n}\n\n.emote-symbol .dropdown-panel .list .emote-item {\n  height: 40px;\n  width: 42px;\n  line-height: 40px;\n  margin: 5px;\n  font-size: 22px;\n  cursor: pointer;\n  text-align: center;\n}\n\n.emote-symbol .dropdown-panel .list .emote-item:hover {\n  background: #eee;\n}\n\n.emote-symbol .dropdown-panel .tabs {\n  border-top: 1px solid #f5f5f5;\n  display: flex;\n}\n.emote-symbol .dropdown-panel .tabs span {\n  padding: 10px 0;\n  border-right: 1px solid #f5f5f5;\n  cursor: pointer;\n  flex: 1;\n}\n.emote-symbol .dropdown-panel .tabs span:last-child {\n  border-right: none;\n}\n\n.emote-symbol .dropdown-panel .tabs .active {\n  color: var(--theme);\n}\n\n/*滚动条样式*/\n.emote-symbol .dropdown-panel .list::-webkit-scrollbar {\n  width: 0px;\n}\n.emote-symbol .dropdown-panel .list::-webkit-scrollbar-track {\n  background: #f5f5f5;\n}\n.emote-symbol .dropdown-panel .list::-webkit-scrollbar-thumb {\n  background: #eee;\n}\n</style>\n"],"mappings":"AAsCA,OAAAA,cAAA;AACA,OAAAC,MAAA;AACA,OAAAC,WAAA;AACA;EACAC,IAAA;EACAC,UAAA;IACAJ;EACA;EACAK,KAAA;IACA;MACAC,MAAA;MACAC,OAAA;MACAC,WAAA;MACAC,YAAA,EAAAP,WAAA;MACAD;IACA;EACA;EACAS,QAAA;IACA,KAAAF,WAAA,GAAAN,WAAA,IAAAS,IAAA;EACA;EACAC,OAAA;IACAC,WAAAC,IAAA,EAAAC,KAAA;MACA,KAAAR,OAAA,GAAAQ,KAAA;MACA,KAAAP,WAAA,GAAAM,IAAA,CAAAH,IAAA;IACA;IACAK,YAAAF,IAAA;MACAG,QAAA,CAAAD,WAAA,CACA,cACA,OACA,2EAAAF,IAAA,wBACA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}